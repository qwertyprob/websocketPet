#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/qwertyprob/js/tools/apps/websocket/node_modules/.pnpm/trpc-cli@0.12.2_@trpc+server@11.8.1_typescript@5.9.3__zod@4.3.5/node_modules/trpc-cli/dist/node_modules:/home/qwertyprob/js/tools/apps/websocket/node_modules/.pnpm/trpc-cli@0.12.2_@trpc+server@11.8.1_typescript@5.9.3__zod@4.3.5/node_modules/trpc-cli/node_modules:/home/qwertyprob/js/tools/apps/websocket/node_modules/.pnpm/trpc-cli@0.12.2_@trpc+server@11.8.1_typescript@5.9.3__zod@4.3.5/node_modules:/home/qwertyprob/js/tools/apps/websocket/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/qwertyprob/js/tools/apps/websocket/node_modules/.pnpm/trpc-cli@0.12.2_@trpc+server@11.8.1_typescript@5.9.3__zod@4.3.5/node_modules/trpc-cli/dist/node_modules:/home/qwertyprob/js/tools/apps/websocket/node_modules/.pnpm/trpc-cli@0.12.2_@trpc+server@11.8.1_typescript@5.9.3__zod@4.3.5/node_modules/trpc-cli/node_modules:/home/qwertyprob/js/tools/apps/websocket/node_modules/.pnpm/trpc-cli@0.12.2_@trpc+server@11.8.1_typescript@5.9.3__zod@4.3.5/node_modules:/home/qwertyprob/js/tools/apps/websocket/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../trpc-cli/dist/bin.js" "$@"
else
  exec node  "$basedir/../trpc-cli/dist/bin.js" "$@"
fi
